{
    "name": "rwos/redox",
    "description": "PHP mappings for Redox Engine",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Woody Gilk",
            "email": "woody.gilk@roundingwell.com"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "RoundingWell\\Redox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RoundingWell\\Redox\\": "tests/",
            "RoundingWell\\Redox\\Generator\\": "generator/"
        }
    },
    "require": {
        "netresearch/jsonmapper": "^1.1"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3",
        "kevinrob/guzzle-cache-middleware": "^2.0",
        "krlove/code-generator": "^1.0",
        "league/flysystem": "^1.0",
        "mtdowling/jmespath.php": "^2.4",
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "clean": [
            "rm -rf src/Fields",
            "rm -rf src/Messages",
            "rm -rf tests/Messages"
        ],
        "generate": "php bin/generate",
        "test": "phpunit"
    }
}
